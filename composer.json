{
    "name": "tenolo/open-graph-bundle",
    "description": "A Symfony OpenGraph bundle",
    "keywords": [
        "opengraph",
        "open",
        "graph",
        "og",
        "symfony",
        "bundle",
        "tenolo"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/tenolo/open-graph-bundle",
    "support": {
        "email": "info@tenolo.de",
        "issues": "https://github.com/tenolo/open-graph-bundle/issues",
        "source": "https://github.com/tenolo/open-graph-bundle/releases",
        "wiki": "https://github.com/tenolo/open-graph-bundle/wiki"
    },
    "authors": [
        {
            "name": "tenolo GmbH & Co. KG",
            "email": "info@tenolo.de",
            "homepage": "https://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "~7.2",

        "symfony/framework-bundle": "~3.4|~4.0",
        "symfony/config": "~3.4|~4.0",
        "symfony/http-kernel": "~3.4|~4.0",
        "symfony/dependency-injection": "~3.4|~4.0",
        "symfony/twig-bundle": "~3.4|~4.0",

        "twig/twig": "~1.42|~2.11|~3.0",
        "euskadi31/opengraph": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\OpenGraphBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
